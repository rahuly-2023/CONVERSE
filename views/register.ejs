<div class="form-bg">
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <div class="form-container">
                    <!-- <div class="form-icon"><i class="fa fa-user"></i></div> -->
                    <h3 class="title">Register</h3>
                    <form  method="POST"  class="form-horizontal" enctype="multipart/form-data">
                        <div class="form-group">
                            <!-- <label>email</label> -->
                            <input class="form-control" type="text" name="name" placeholder="Email Name" required>
                        </div>
                        <div class="form-group">
                            <!-- <label>password</label> -->
                            <input class="form-control" type="email" name="email" placeholder="Enter Email"  required>
                        </div>
                        <div class="form-group">
                            <!-- <label>password</label> -->
                            <input class="form-control" type="file" name="image"  required>
                        </div>
                        <div class="form-group">
                            <!-- <label>password</label> -->
                            <input class="form-control" type="password" name="password" placeholder="Enter Password"  required>
                        </div>
                        <button type="submit" class="btn btn-default" style="cursor:pointer;">Register</button>
                        
                        
                        <br><br>
                        <button type="button" class="btn btn-default" fdprocessedid="1yxilo">
                            <a href="/login">Login</a>
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>














